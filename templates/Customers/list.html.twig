{% extends '/_base.html.twig' %}

{% block pageTitle %}Customers list page{% endblock %}

{% block main %}

    <h1>Customer list</h1>

    <table>
        {% for customer in customers %}
            <tr>
                <th>customerName</th>
                <th>addressLine1</th>
                <th>addressLine2</th>
                <th>addressLine3</th>
                <th>county/ postcode</th>
                <th>officeName</th>
                <th>mobileTelephone</th>
                <th>landlineTelephone</th>
            </tr>

            <tr>
                <td>{{ customer.customerName}}</td>
                <td>{{ customer.addressLine1}}</td>
                <td>{{ customer.addressLine2}}</td>
                <td>{{ customer.addressLine3}}</td>
                <td>{{ customer.countyPostcodeName}}</td>
                <td>{{ customer.officeName}}</td>
                <td>{{ customer.mobileTelephone}}</td>
                <td>{{ customer.landlineTelephone}}</td>
            </tr>
        {% endfor %}
    </table>

{% endblock %}